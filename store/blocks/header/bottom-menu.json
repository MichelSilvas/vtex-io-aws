{
  "vtex.menu@2.x:menu#websites": {
    "children": [
      "menu-item#logo",
      "menu-item#about-us"
    ]
  },
  "menu-item#logo": {
    "props": {
      "id": "menu-item-logo",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "url": "./assets/logo.jpg",
        "noFollow": false,
         "size": "70"
      }
    }
  },
  "vtex.menu@2.x:menu#institutional": {
    "children": [
      "menu-item#vtex-website"
    ]
  },
  "menu-item#vtex-website": {
    "props": {
      "id": "menu-item-vtex-website",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "external",
        "href": "http://vtex.com",
        "noFollow": false,
        "tagTitle": "visit vtex.com",
        "text": "visit vtex.com"
      }
    }
  }
}
